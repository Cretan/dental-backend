{
  "kind": "collectionType",
  "collectionName": "pacients",
  "info": {
    "singularName": "pacient",
    "pluralName": "pacients",
    "displayName": "Pacient"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nume": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "prenume": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "cnp": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 13
    },
    "data_nasterii": {
      "type": "date",
      "required": true
    },
    "adresa": {
      "type": "string",
      "maxLength": 300
    },
    "telefon": {
      "type": "string",
      "required": true,
      "maxLength": 15
    },
    "email": {
      "type": "email",
      "maxLength": 254
    },
    "istoric_medical": {
      "type": "blocks"
    },
    "alergii": {
      "type": "string",
      "maxLength": 500
    },
    "cabinet": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cabinet.cabinet",
      "inversedBy": "pacienti"
    },
    "planuri_tratamente": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::plan-tratament.plan-tratament",
      "mappedBy": "pacient"
    },
    "vizite": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vizita.vizita",
      "mappedBy": "pacient"
    },
    "status_pacient": {
      "type": "enumeration",
      "enum": [
        "Activ",
        "Inactiv",
        "Arhivat"
      ],
      "default": "Activ"
    },
    "facturi": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::factura.factura",
      "mappedBy": "pacient"
    },
    "plati": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::plata.plata",
      "mappedBy": "pacient"
    },
    "added_by": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
