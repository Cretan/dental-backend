      "enum": [
        "Create",
        "Update",
        "Delete",
        "View"
      ]
    },
    "entitate": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "entitate_id": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "date_vechi": {
