{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nume": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "prenume": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "specializare": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Generala",
        "Ortodontie",
        "Endodontie",
        "Chirurgie",
        "Parodontologie",
        "Pedodontie",
        "Estetica"
      ]
    },
    "nr_licenta": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 50
    },
    "telefon": {
      "type": "string",
      "maxLength": 15
    },
    "email": {
      "type": "email",
      "maxLength": 254
    },
    "program_lucru": {
      "type": "json"
    },
    "cabinet": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cabinet.cabinet",
      "inversedBy": "doctors"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "added_by": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
