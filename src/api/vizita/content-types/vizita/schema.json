{
  "kind": "collectionType",
  "collectionName": "vizitas",
  "info": {
    "singularName": "vizita",
    "pluralName": "vizitas",
    "displayName": "Vizita"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cabinet": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cabinet.cabinet"
    },
    "pacient": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pacient.pacient",
      "inversedBy": "vizite"
    },
    "data_programare": {
      "type": "datetime",
      "required": true,
      "unique": false
    },
    "tip_vizita": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "VizitaInitiala",
        "PlanTratament",
        "Programare"
      ]
    },
    "durata": {
      "type": "integer",
      "default": 60
    },
    "status_vizita": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Programata",
        "Confirmata",
        "Finalizata",
        "Anulata"
      ]
    },
    "tratamente": {
      "type": "component",
      "component": "tratament-dinte.tratament-dinte",
      "repeatable": true
    },
    "observatii": {
      "type": "string",
      "maxLength": 2000
    },
    "medic": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::doctor.doctor"
    },
    "added_by": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "radiografii": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::radiografie.radiografie",
      "mappedBy": "vizita"
    }
  }
}
