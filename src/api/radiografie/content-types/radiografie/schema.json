    },
    "pacient": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pacient.pacient",
      "inversedBy": "radiografii"
    },
    "vizita": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vizita.vizita",
